  <DataSets>
    <DataSet Name="Accounting">
      <Query>
        <DataSourceName>Galactic</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        Customer.CustomerNumber, Customer.Name, Customer.BillingCity,                              Customer.BillingEmail, InvoiceHeader.InvoiceNumber, Payment.Amount, Payment.PaymentDate
FROM            Customer INNER JOIN
                         InvoiceHeader ON Customer.CustomerNumber = InvoiceHeader.CustomerNumber INNER JOIN
                         Payment ON Customer.CustomerNumber = Payment.CustomerNumber
WHERE        (Payment.Amount &gt; 300.00) AND (Payment.PaymentDate &gt;= @StartDate) AND (Payment.PaymentDate &lt; DATEADD(dd, 1, @EndDate))
ORDER BY Customer.Name, InvoiceHeader.InvoiceNumber</CommandText>
      </Query>
      <Fields>
        <Field Name="CustomerNumber">
          <DataField>CustomerNumber</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="BillingCity">
          <DataField>BillingCity</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="BillingEmail">
          <DataField>BillingEmail</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="InvoiceNumber">
          <DataField>InvoiceNumber</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Amount">
          <DataField>Amount</DataField>
